<?xml version="1.0"?><project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.codehaus.swizzle</groupId>
  <artifactId>swizzle-stream</artifactId>
  <name>Swizzle :: Stream</name>
  <version>1.0.1</version>
  <url>http://swizzle.codehaus.org</url>
  <build>
    <extensions>
      <extension>
        <groupId>org.apache.maven.wagon</groupId>
        <artifactId>wagon-webdav</artifactId>
        <version>1.0-beta-1</version>
      </extension>
    </extensions>
  </build>
  <dependencies>
    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>
  </dependencies>
  <distributionManagement>
    <repository>
      <id>codehaus.org</id>
      <name>Swizzle Central Repository</name>
      <url>dav:https://dav.codehaus.org/repository/swizzle/</url>
    </repository>
    <snapshotRepository>
      <id>codehaus.org</id>
      <name>Swizzle Central Development Repository</name>
      <url>dav:https://dav.codehaus.org/snapshots.repository/swizzle/</url>
    </snapshotRepository>
    <site>
      <id>codehaus.org</id>
      <url>dav:https://dav.codehaus.org/swizzle/</url>
    </site>
    <status>deployed</status>
  </distributionManagement>
</project>